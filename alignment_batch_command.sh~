samtools flagstat 7047-03-11_S3_L007_001_bwa.bam >7047-03-11_S3_L007_001_mapped_bwa_summary
bowtie2 -p 18 --sensitive --dpad 0 --gbar 99999999 --mp 1,1 --np 1 --no-mixed --no-discordant -x ../Genomes_assembled/Penicillium_bowtie2_ref -1 7047-03-11_S3_L007_R1_001.fastq.gz -2 7047-03-11_S3_L007_R2_001.fastq.gz -S 7047-03-11_S3_L007_001_bowtie2.sam    
/Data/genome_assembly/RSEM-master/rsem-calculate-expression --no-bam-output -p 18 --sam --paired-end ./7047-03-11_S3_L007_001_bowtie2.sam ../Genomes_assembled/Penicillium_bowtie2_ref 7047-03-11_S3_L007_001_bowtie2F                      
samtools view -bS 7047-03-11_S3_L007_001_bowtie2.sam > 7047-03-11_S3_L007_001_bowtie2.bam                      
samtools flagstat 7047-03-11_S3_L007_001_bowtie2.bam >7047-03-11_S3_L007_001_mapped_bowtie2_summary 

